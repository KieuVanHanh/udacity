{
	"name": "dataflow_Summary_NYC_PAYROLL _2020_2021",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "NYC_Payroll_DATA_2020",
						"type": "DatasetReference"
					},
					"name": "NYCPayrollDATA2020"
				},
				{
					"dataset": {
						"referenceName": "NYC_Payroll_DATA_2021",
						"type": "DatasetReference"
					},
					"name": "NYCPayrollDATA2021"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "NYC_Payroll_Summary",
						"type": "DatasetReference"
					},
					"name": "payrollSummarySQL"
				},
				{
					"dataset": {
						"referenceName": "NYCPayrollSummary",
						"type": "DatasetReference"
					},
					"name": "payrollsummaryStorage"
				}
			],
			"transformations": [
				{
					"name": "union2020And2021"
				},
				{
					"name": "filterYear"
				},
				{
					"name": "totalPaid"
				},
				{
					"name": "aggregate"
				}
			],
			"scriptLines": [
				"parameters{",
				"     fiscalyear_param as integer (2020)",
				"}",
				"source(output(",
				"          FiscalYear as integer,",
				"          PayrollNumber as integer,",
				"          AgencyID as string,",
				"          AgencyName as string,",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string,",
				"          AgencyStartDate as date,",
				"          WorkLocationBorough as string,",
				"          TitleCode as string,",
				"          TitleDescription as string,",
				"          LeaveStatusasofJune30 as string,",
				"          BaseSalary as double,",
				"          PayBasis as string,",
				"          RegularHours as double,",
				"          RegularGrossPaid as double,",
				"          OTHours as double,",
				"          TotalOTPaid as double,",
				"          TotalOtherPay as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> NYCPayrollDATA2020",
				"source(output(",
				"          FiscalYear as integer,",
				"          PayrollNumber as integer,",
				"          AgencyCode as string,",
				"          AgencyName as string,",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string,",
				"          AgencyStartDate as date,",
				"          WorkLocationBorough as string,",
				"          TitleCode as string,",
				"          TitleDescription as string,",
				"          LeaveStatusasofJune30 as string,",
				"          BaseSalary as double,",
				"          PayBasis as string,",
				"          RegularHours as double,",
				"          RegularGrossPaid as double,",
				"          OTHours as double,",
				"          TotalOTPaid as double,",
				"          TotalOtherPay as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> NYCPayrollDATA2021",
				"NYCPayrollDATA2020, NYCPayrollDATA2021 union(byName: true)~> union2020And2021",
				"union2020And2021 filter(toInteger(FiscalYear) >= $fiscalyear_param) ~> filterYear",
				"filterYear derive(totalpaid = RegularGrossPaid + TotalOTPaid  +TotalOtherPay) ~> totalPaid",
				"totalPaid aggregate(groupBy(AgencyName,",
				"          FiscalYear),",
				"     SumTotalPaid = sum(totalpaid)) ~> aggregate",
				"aggregate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          FiscalYear as integer,",
				"          AgencyName as string,",
				"          TotalPaid as double",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          FiscalYear,",
				"          AgencyName,",
				"          TotalPaid = SumTotalPaid",
				"     )) ~> payrollSummarySQL",
				"aggregate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> payrollsummaryStorage"
			]
		}
	}
}